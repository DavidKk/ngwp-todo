{
  "name": "ngwp-todo",
  "version": "3.0.0-beta",
  "license": "MIT",
  "description": "Angular 1.x.x scaffold with ngwp",
  "engines": {
    "node": ">= 6.2.2"
  },
  "scripts": {
    "standard": "standard --fix './src/**/*.js' './test/**/*.js' --parser babel-eslint",
    "nginx": "ngwp nginx",
    "develop": "ngwp run develop",
    "product": "ngwp run product",
    "e2e": "ngwp run e2e",
    "coverage": "rimraf ./coverage && cross-env COVERAGE=1 ngwp run e2e",
    "release": "npm run e2e && npm run product",
    "start": "npm run develop"
  },
  "peerDependencies": {
    "ngwp": "^3.0.0"
  },
  "dependencies": {
    "angular": "1.6.3",
    "angular-ui-router": "^0.4.3",
    "bourbon": "^5.0.0",
    "lodash": "^4.14.0",
    "ng-redux": "^3.5.2",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8",
    "todomvc-app-css": "^2.1.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.3",
    "babel-eslint": "^8.0.1",
    "cross-env": "^5.0.5",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  }
}
